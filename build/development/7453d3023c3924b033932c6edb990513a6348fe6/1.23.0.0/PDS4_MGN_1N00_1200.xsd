     <xs:minInclusive value="-1.7976931348623157e308"/>
     <xs:maxInclusive value="1.7976931348623157e308"/>
	   </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="orbit_semimajor">
    <xs:annotation>
      <xs:documentation>Orbit semi-major axis from the Orbit Header file
